{
  "functions": {
    "app/api/widget/[slug]/embed/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Referrer-Policy",
          "value": "no-referrer"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://www.notion.so https://notion.so https://*.notion.so https://*.notion.site"
        }
      ]
    },
    {
      "source": "/api/widget/(.*)/embed",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://www.notion.so https://notion.so https://*.notion.so https://*.notion.site"
        }
      ]
    },
    {
      "source": "/embed/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://www.notion.so https://notion.so https://*.notion.so https://*.notion.site"
        }
      ]
    },
    {
      "source": "/w/(.*)/embed-static",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://www.notion.so https://notion.so https://*.notion.so https://*.notion.site"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=300"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/embed/:slug",
      "destination": "/api/widget/:slug/embed"
    },
    {
      "source": "/w/:slug.svg",
      "destination": "/w/:slug/snapshot"
    }
  ]
}
